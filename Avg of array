public class main {
    public static void main(String[] args)
    {   int a = 0 ;
        float n;
        int[] numbers = {1, 2, 3, 4, 5,6,7};
        System.out.print("Avg of all Array elements are: ");
        for ( int i = 0; i < numbers.length; i++)
        a = a + numbers[i];
        n = a / numbers.length ;
        System.out.println(n);
        
    }
}
