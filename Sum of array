public class main {
    public static void main(String[] args)
    {   int a = 0 ;
        int[] numbers = {1, 2, 3, 4, 5,6,7};
        System.out.print("Sum of all Array elements are: ");
        for ( int i = 0; i < numbers.length; i++)
        a = a + numbers[i];
        System.out.println(a);
        
    }
}
