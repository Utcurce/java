public class main {
    public static void main(String[] args)
    {   int max , min ;
        
        int[] a = {1, 2, 3, 4, 5,6,7};
        max = a[0];
        min = a[0];
        for ( int i = 0; i < a.length; i++)
        {
            if(max<a[i])
            max = a[i];
            if(min>a[i])
            min = a[i];
        }
        System.out.println("Maximum no. is : " + max);
        System.out.println("Minimum no. is : " + min);
         }
}
